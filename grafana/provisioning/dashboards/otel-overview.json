    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "description": "Service dependency graph showing calls between services. Nodes represent services, edges represent calls with request rate.",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "nodeGraph": {
              "arcs": [
                { "color": "green", "field": "arc__success" },
                { "color": "red", "field": "arc__errors" }
              ]
            }
          }
        }
      },
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 35 },
      "id": 11,
      "options": {
        "nodes": {},
        "edges": {}
