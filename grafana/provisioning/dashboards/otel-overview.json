        {
          "expr": "sum by (service_name) (rate(${namespace}_calls_total[$__rate_interval]))",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "nodes"
        },
        {
          "expr": "sum by (service_name) (rate(${namespace}_calls_total{http_response_status_code!~\"5..\"}[$__rate_interval]))",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "nodes_success"
